#: src/main.cpp:120
#, c-format
msgid "Usage: %s [OPTION]..."
msgstr "Utilisation : %s [OPTION]..."

#: src/main.cpp:122
#, c-format
msgid "The best game in the world!"
msgstr "Le meilleur jeu du monde !"

#: src/main.cpp:125
#, c-format
msgid "-h, --help display this help and exit."
msgstr "-h, --help affiche cette aide et quitte."

#: src/main.cpp:128
#, c-format
msgid "-s, --server <IP> specify the ip of the server you want to join."
msgstr ""
"-s, --server <IP> spécifie l'adresse IP du serveur que l'on souhaite "
"rejoindre."

#: src/main.cpp:130
#, c-format
msgid ""
"-p, --server-port <port> specify the port of the server you want to join "
"(default: 4284)."
msgstr ""
"-p, --server-port <port> spécifie le port du serveur que l'on souhaite "
"rejoindre."

#: src/main.cpp:145
#, c-format
msgid ""
